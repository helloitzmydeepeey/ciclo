<script>
function selectOptionByValue(selectId, value) {
    var dropdown = document.getElementById(selectId);

    for (var i = 0; i < dropdown.options.length; i++) {
        if (dropdown.options[i].value === value) {
            dropdown.options[i].selected = true;

<script>
function selectOptionByValue(selectId, value) {
    var dropdown = document.getElementById(selectId);

    for (var i = 0; i < dropdown.options.length; i++) {
        if (dropdown.options[i].value === value) {
            dropdown.options[i].selected = true;
            break;
        }
    }
}

function calculateTenure() {
    var selectedDate = document.getElementById("source_of_income_private_employee_date_hired").value;
    var dateHired = new Date(selectedDate);
    var currentDate = new Date();

    var timeDiff = Math.abs(currentDate.getTime() - dateHired.getTime());
    var diffDays = Math.ceil(timeDiff / (1000 * 3600 * 24));

    // Example logic, adjust based on your business rules
    if (diffDays < 180) {
        selectOptionByValue("source_of_income_private_employee_tenure", "5011"); // < 6 MONTHS
    } else if (diffDays < 365) {
        selectOptionByValue("source_of_income_private_employee_tenure", "5009"); // 6 MOS. - 1 YEAR
    } else if (diffDays < 730) {
        selectOptionByValue("source_of_income_private_employee_tenure", "5013"); // 1-2 YEARS
    } else if (diffDays < 1095) {
        selectOptionByValue("source_of_income_private_employee_tenure", "5010"); // 2-3 YEARS
    } else if (diffDays < 1825) {
        selectOptionByValue("source_of_income_private_employee_tenure", "5014"); // 3-5 YEARS
    } else {
        selectOptionByValue("source_of_income_private_employee_tenure", "5012"); // > 5 YEARS
    }
}

// Call calculateTenure when the date is changed
document.getElementById("source_of_income_private_employee_date_hired").onchange = calculateTenure;


